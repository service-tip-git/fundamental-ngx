{
    "name": "core-custom-toolbar",
    "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/core/src/lib/custom-toolbar",
    "targets": {
        "build": {
            "executor": "@nx/angular:ng-packagr-lite",
            "outputs": ["{workspaceRoot}/dist/libs/core/custom-toolbar"],
            "options": {
                "project": "libs/core/src/lib/custom-toolbar/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/core/src/lib/custom-toolbar/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/core/src/lib/custom-toolbar/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/core/src/lib/custom-toolbar/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        }
    },
    "tags": ["scope:fd", "type:lib"]
}
